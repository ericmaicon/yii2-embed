{
    "name": "ericmaicon/yii2-embed",
    "description": "Just a simple yii2 embed component.",
    "keywords": ["yii", "extension", "embed"],
    "homepage": "https://github.com/ericmaicon/yii2-embed",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Eric Maicon",
            "email": "eric@ericmaicon.com.br"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "2.0.4",
        "yiisoft/yii2-bootstrap": "2.0.4",
        "embed/embed": "2.5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "embed\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}